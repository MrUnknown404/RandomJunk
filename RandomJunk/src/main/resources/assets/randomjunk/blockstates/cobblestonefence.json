{
    "multipart": [
        {   "apply": { "model": "randomjunk:fences/cobblestonefence_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "randomjunk:fences/cobblestonefence_side", "uvlock": true }
        },
        {	"when": { "east": "true" },
            "apply": { "model": "randomjunk:fences/cobblestonefence_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "randomjunk:fences/cobblestonefence_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "randomjunk:fences/cobblestonefence_side", "y": 270, "uvlock": true }
        }
    ]
}
